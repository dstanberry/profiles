{
  // **************************************************************************
  // ** EXTENSIONS
  // **************************************************************************
  "code-eol.style": {
    "code-eol.crlfCharacter": "↵",
    "code-eol.newlineCharacter": "¬",
    "code-eol.returnCharacter": "⇠",
    "code-eol.validLineEnding": "LF",
    "color": "#5a5a5a",
    "opacity": 0.4
  },
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.recentChange.enabled": false,
  "gitlens.currentLine.enabled": false,
  "prettier.arrowParens": "always",
  "prettier.bracketSpacing": true,
  "prettier.endOfLine": "lf",
  "prettier.insertPragma": false,
  "prettier.printWidth": 120,
  "prettier.semi": true,
  "prettier.singleQuote": false,
  "prettier.tabWidth": 2,
  "prettier.trailingComma": "es5",
  "prettier.useTabs": false,
  "shellformat.flag": "-i 2 -bn -ci -sr",
  "todohighlight.isEnable": true,
  "todohighlight.isCaseSensitive": true,
  "todohighlight.keywords": [
    {
      "text": "ERROR:",
      "color": "#c9d1d9",
      "backgroundColor": "#bf616a",
      "overviewRulerColor": "#bf616a",
      "fontWeight": "bold"
    },
    {
      "text": "WARN:",
      "color": "#59595e",
      "backgroundColor": "#cd8d88",
      "fontWeight": "bold"
    },
    {
      "text": "TODO:",
      "color": "#c9d1d9",
      "backgroundColor": "#62757e",
      "fontWeight": "bold"
    },
    {
      "text": "NOTE:",
      "color": "#c9d1d9",
      "backgroundColor": "#b48ead",
      "fontWeight": "bold"
    },
    {
      "text": "TEST:",
      "color": "#c9d1d9",
      "backgroundColor": "#7f966d",
      "fontWeight": "bold"
    }
  ],
  "vim.leader": "<space>",
  "vim.useCtrlKeys": true,
  "vim.hlsearch": true,
  "vim.highlightedyank.enable": true,
  "vim.useSystemClipboard": true,
  "vim.cursorStylePerMode.normal": "block",
  "vim.cursorStylePerMode.insert": "line-thin",
  "vim.cursorStylePerMode.replace": "block-outline",
  "vim.statusBarColorControl": false,
  "vim.insertModeKeyBindings": [
    {
      "before": ["j", "k"],
      "after": ["<esc>"]
    }
  ],
  "vim.normalModeKeyBindings": [
    {
      "before": ["<cr>"],
      "commands": [":set hlsearch!"]
    },
    {
      "before": ["<left>"],
      "commands": [":bprevious"]
    },
    {
      "before": ["<right>"],
      "commands": [":bnext"]
    },
    {
      "before": ["f", "f"],
      "commands": ["editor.action.formatDocument"]
    },
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["g", "_"]
    },
    {
      "before": ["v", "a", "g"],
      "after": ["g", "g", "V", "G", "g", "_"]
    }
  ],
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": [";"],
      "after": [":"]
    },
    {
      "before": ["<up>"],
      "after": ["<c-y>"]
    },
    {
      "before": ["<down>"],
      "after": ["<c-e>"]
    },
    {
      "before": ["p"],
      "after": ["p", "g", "v", "y"]
    },
    {
      "before": ["J"],
      "after": ["m", "z", "J", "`", "z"]
    }
  ],
  "vim.operatorPendingModeKeyBindings": [
    {
      "before": ["H"],
      "after": ["^"]
    },
    {
      "before": ["L"],
      "after": ["$"]
    }
  ],
  "vim.visualModeKeyBindings": [
    {
      "before": ["<"],
      "commands": ["editor.action.outdentLines"]
    },
    {
      "before": [">"],
      "commands": ["editor.action.indentLines"]
    }
  ],
  // **************************************************************************
  // ** EDITOR
  // **************************************************************************
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit",
    "source.organizeImports": "explicit"
  },
  "editor.guides.bracketPairs": "active",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorStyle": "line",
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 1,
  "editor.detectIndentation": true,
  "editor.fontFamily": "CartographCF Nerd Font",
  "editor.fontLigatures": true,
  "editor.fontSize": 13,
  "editor.fontWeight": "300",
  "editor.guides.indentation": true,
  "editor.inlayHints.enabled": "on",
  "editor.inlayHints.fontSize": 8,
  "editor.insertSpaces": true,
  "editor.lineHeight": 22,
  "editor.lineNumbers": "relative",
  "editor.matchBrackets": "never",
  "editor.minimap.enabled": false,
  "editor.mouseWheelZoom": true,
  "editor.quickSuggestions": {
    "other": "inline",
    "comments": "off",
    "strings": "inline"
  },
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [80, 120],
  "editor.semanticHighlighting.enabled": true,
  "editor.stickyScroll.enabled": true,
  "editor.suggestSelection": "first",
  "editor.tabSize": 2,
  "editor.wordWrap": "wordWrapColumn",
  "editor.wordWrapColumn": 120,
  "editor.wrappingIndent": "same",
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  // **************************************************************************
  // ** LANGUAGES
  // **************************************************************************
  "emmet.includeLanguages": {
    "javascript": "html"
  },
  "flake8.args": [],
  "html.format.wrapLineLength": 240,
  "html.format.preserveNewLines": true,
  "html.format.indentInnerHtml": false,
  "html.format.wrapAttributes": "auto",
  "html.format.enable": false,
  "markdown.preview.breaks": true,
  // **************************************************************************
  // ** CODELENS
  // **************************************************************************
  "html.autoClosingTags": false,
  "html.suggest.html5": true,
  "html.validate.scripts": true,
  "html.validate.styles": true,
  "javascript.inlayHints.functionLikeReturnTypes.enabled": true,
  "javascript.inlayHints.parameterNames.enabled": "all",
  "javascript.inlayHints.parameterTypes.enabled": true,
  "javascript.inlayHints.variableTypes.enabled": true,
  "javascript.referencesCodeLens.enabled": true,
  "javascript.updateImportsOnFileMove.enabled": "prompt",
  "typescript.inlayHints.functionLikeReturnTypes.enabled": true,
  "typescript.inlayHints.parameterNames.enabled": "all",
  "typescript.inlayHints.parameterTypes.enabled": true,
  "typescript.inlayHints.variableTypes.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "prompt",
  // **************************************************************************
  // ** FTDETECT
  // **************************************************************************
  "files.associations": {
    "Dockerfile-*": "dockerfile",
    "Dockerfile_*": "dockerfile"
  },
  "files.autoSave": "off",
  "files.autoSaveDelay": 10000,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/node_modules": true
  },
  // **************************************************************************
  // ** GIT
  // **************************************************************************
  "git.autofetch": true,
  "git.defaultCloneDirectory": "D:\\Git",
  "git.enableSmartCommit": true,
  "git.confirmSync": false,
  "git.mergeEditor": true,
  "git.useEditorAsCommitInput": true,
  "git.terminalGitEditor": true,
  // **************************************************************************
  // ** FTPLUGIN
  // **************************************************************************
  "[c]": {
    "editor.defaultFormatter": "ms-vscode.cpptools"
  },
  "[css]": {
    "editor.suggest.insertMode": "replace"
  },
  "[gopls]": {
    "ui.semanticTokens": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.foldingStrategy": "indentation"
  },
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll": "never",
      "source.organizeImports": "explicit"
    },
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": false,
    "editor.formatOnType": false
  },
  "[markdown]": {
    "editor.lineHeight": 24,
    "editor.overviewRulerBorder": false,
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    },
    "editor.wrappingIndent": "indent",
    "files.trimTrailingWhitespace": false
  },
  "[python]": {
    "editor.defaultFormatter": "ms-python.black-formatter",
    "editor.formatOnSave": true
  },
  // **************************************************************************
  // ** TERMINAL
  // **************************************************************************
  "terminal.external.windowsExec": "wt.exe",
  "terminal.integrated.fontFamily": "CartographCF Nerd Font",
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontWeight": "400",
  "terminal.integrated.minimumContrastRatio": 1,
  "terminal.integrated.showExitAlert": false,
  "terminal.integrated.splitCwd": "workspaceRoot",
  "terminal.integrated.profiles.windows": {
    "PowerShell": {
      "source": "PowerShell",
      "icon": "terminal-powershell"
    },
    "Command Prompt": {
      "path": ["${env:windir}\\Sysnative\\cmd.exe", "${env:windir}\\System32\\cmd.exe"],
      "args": [],
      "icon": "terminal-cmd"
    },
    "Git Bash": null
  },
  "terminal.integrated.profiles.linux": {
    "bash": {
      "path": "bash",
      "icon": "terminal-linux"
    },
    "zsh": {
      "path": "zsh",
      "icon": "terminal-linux"
    },
    "tmux": {
      "path": "tmux",
      "icon": "terminal-tmux"
    }
  },
  "terminal.integrated.smoothScrolling": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.windows": "PowerShell",
  "powershell.promptToUpdatePowerShell": false,
  // **************************************************************************
  // ** HARDENING
  // **************************************************************************
  "security.workspace.trust.untrustedFiles": "open",
  "telemetry.telemetryLevel": "off",
  "update.mode": "manual",
  // **************************************************************************
  // ** WORKBENCH
  // **************************************************************************
  "window.commandCenter": true,
  "window.menuBarVisibility": "compact",
  "window.title": "",
  "workbench.colorTheme": "Catppuccin Frappé",
  "workbench.colorCustomizations": {
    "[KDark]": {
      "editor.selectionBackground": "#49535e",
      "editor.wordHighlightBackground": "#00000000",
      "editorCursor.foreground": "#e5c179",
      "editorInlayHint.background": "#37373700",
      "editorInlayHint.foreground": "#8f8f8f",
      "editorRuler.foreground": "#5a5a5a20",
      "tab.activeBackground": "#37373700",
      "tab.activeForeground": "#d8dee9"
    },
    "editorBracketHighlight.foreground1": "#73c1b9",
    "editorBracketHighlight.foreground2": "#80d3dd",
    "editorBracketHighlight.foreground3": "#9086a4",
    "editorBracketHighlight.foreground4": "#bfafc4",
    "editorBracketHighlight.foreground5": "#dec7d0",
    "editorBracketHighlight.foreground6": "#e09696",
    "editorBracketHighlight.unexpectedBracket.foreground": "#ffdca8",
    "statusBar.background": "#5f00af",
    "statusBar.noFolderBackground": "#5f00af",
    "statusBar.debuggingBackground": "#5f00af",
    "statusBar.foreground": "#ffffff",
    "statusBar.debuggingForeground": "#ffffff"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook"
  },
  "workbench.iconTheme": "catppuccin-frappe",
  "workbench.layoutControl.enabled": false,
  "workbench.productIconTheme": "icons-carbon",
  "workbench.settings.editor": "json",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tips.enabled": false
}

